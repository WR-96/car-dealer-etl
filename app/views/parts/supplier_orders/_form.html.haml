    = f.text_field :employee_name
  .field
    = f.label :date
    = f.text_field :date
  .field
    = f.label :supplier_tradename
    = f.text_field :supplier_tradename
  .field
    = f.label :status
    = f.text_field :status
  .field
    = f.label :delivery_date
    = f.text_field :delivery_date
  .field
    = f.label :article
    = f.text_field :article
  .field
    = f.label :price
    = f.text_field :price
  .field
    = f.label :amount
    = f.text_field :amount
  .field
    = f.label :total
    = f.text_field :total
  .actions
    = f.submit 'Save'
