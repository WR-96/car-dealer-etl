= form_for @parts_provider do |f|
  - if @parts_provider.errors.any?
    #error_explanation
      %h2= "#{pluralize(@parts_provider.errors.count, "error")} prohibited this parts_provider from being saved:"
      %ul
        - @parts_provider.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :tradename
    = f.text_field :tradename
  .field
    = f.label :phone
    = f.text_field :phone
  .field
    = f.label :cellphone
    = f.text_field :cellphone
  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :car_maker
    = f.text_field :car_maker
  .field
    = f.label :piece
    = f.text_field :piece
  .field
    = f.label :price
    = f.text_field :price
  .actions
    = f.submit 'Save'
